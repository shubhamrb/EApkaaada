<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="160dp"
    android:id="@+id/viewClick"
    android:layout_height="wrap_content"
    android:padding="0sp"
    android:elevation="1dp"
    android:layout_margin="@dimen/Margin5dp"
    android:gravity="center"
    android:layout_gravity="center"
    android:background="@drawable/white_gredient_tobf_bg"


    android:orientation="vertical">
    <FrameLayout
        android:id="@+id/frameId"
        android:layout_width="160dp"
        android:layout_height="130sp"
        android:layout_gravity="center"
        app:layout_constraintTop_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="0sp">

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="130sp"
            android:contentDescription=""
            android:scaleType="fitXY"
            android:src="@drawable/f"
            app:shapeAppearanceOverlay="@style/OneSideRoundedImageRect"
            android:adjustViewBounds="true"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:layout_margin="8sp"
          android:padding="@dimen/Margin5dp"
            android:background="@drawable/white_gredient_circle"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/likeDislike"
                android:layout_width="15sp"
                android:layout_gravity="center"
                android:gravity="center"
                android:backgroundTint="@color/gray_heart"

                android:layout_height="15sp"
                android:background="@drawable/heart"

                />
            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/hearAnimation"
                android:layout_width="15sp"
                android:layout_height="15sp"
                android:visibility="gone"

                android:layout_gravity="center"
                android:gravity="center"
                app:lottie_rawRes="@raw/heartlike"
                app:lottie_autoPlay="true"
                app:lottie_loop="true"/>
        </androidx.appcompat.widget.LinearLayoutCompat>

    </FrameLayout>
<androidx.appcompat.widget.LinearLayoutCompat
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/Margin5dp"
    android:layout_height="wrap_content">
    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tvName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/forget_password"
        app:fontFamily="@font/montserrat_b"
        android:gravity="center|start"



        android:fontFamily="@font/montserrat_b"
        android:textSize="@dimen/twelve"

        android:textColor="@color/black"

        app:layout_constraintTop_toBottomOf="@id/frameId"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tvDescription"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/forget_password"
        android:fontFamily="@font/montserrat"
        app:fontFamily="@font/montserrat"
        android:gravity="center|start"
android:maxLines="1"


        android:textSize="@dimen/twelve"

        android:textColor="@color/black"

        app:layout_constraintTop_toBottomOf="@id/frameId"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:weightSum="5"

        android:layout_height="wrap_content">
<androidx.appcompat.widget.LinearLayoutCompat
    android:layout_width="match_parent"
   android:layout_weight="1.4"
    android:layout_marginStart="@dimen/Margin10dp"
    android:orientation="horizontal"
    android:layout_gravity="center"
    android:gravity="center|start"
    android:layout_height="wrap_content">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/starOne"
        android:layout_width="16sp"
        android:background="@drawable/ic_star_black_24dp"
        android:backgroundTint="@color/gray"
        android:layout_height="16sp"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="16sp"
        android:id="@+id/starTwo"
        android:background="@drawable/ic_star_black_24dp"
        android:backgroundTint="@color/gray"
        android:layout_height="16sp"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="16sp"
        android:id="@+id/starThree"
        android:background="@drawable/ic_star_black_24dp"
        android:backgroundTint="@color/gray"
        android:layout_height="16sp"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="16sp"
        android:id="@+id/starFour"
        android:background="@drawable/ic_star_black_24dp"
        android:backgroundTint="@color/gray"
        android:layout_height="16sp"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="16sp"
        android:id="@+id/starFive"
        android:background="@drawable/ic_star_black_24dp"
        android:backgroundTint="@color/gray"
        android:layout_height="16sp"/>

 </androidx.appcompat.widget.LinearLayoutCompat>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_weight="3.7"
            android:layout_gravity="center"
            android:gravity="center|start"
            android:layout_height="wrap_content">
<com.google.android.material.textview.MaterialTextView
    android:layout_width="wrap_content"
    android:text="Good"
    android:textSize="13sp"
    android:textColor="@color/green"


    android:layout_height="wrap_content"/>
        </androidx.appcompat.widget.LinearLayoutCompat>

    </androidx.appcompat.widget.LinearLayoutCompat>
<View
    android:layout_width="match_parent"
    android:layout_height="@dimen/Margin10dp"/>
</androidx.appcompat.widget.LinearLayoutCompat>
